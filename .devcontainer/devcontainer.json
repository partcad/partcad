{
  "name": "PartCAD Dev Container",
  "build": { // 13:27.664
    "dockerfile": "./Dockerfile"
  },
  "containerEnv": {
    "PRE_COMMIT_HOME": "/workspaces/partcad/.pre-commit"
  },
  "features": {
    "ghcr.io/devcontainers/features/python:1": { // 06:25.200
      // https://github.com/devcontainers/features/tree/main/src/python
      "version": "3.10",
      "installTools": true,
      "toolsToInstall": "pre-commit,poetry",
      "optimize": true,
      "enableShared": false,
      "installPath": "/usr/local/python",
      "installJupyterlab": false,
      "configureJupyterlabAllowOrigin": "",
      "httpProxy": ""
    },
    "ghcr.io/devcontainers/features/conda:1": { // 00:35.700
      // https://github.com/devcontainers/features/tree/main/src/conda
      "version": "latest",
      "addCondaForge": false
    }
  },
  "postCreateCommand": ".devcontainer/postCreateCommand.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "bernhard-42.ocp-cad-viewer",
        "bierner.github-markdown-preview",
        "bierner.markdown-checkbox",
        "bierner.markdown-footnotes",
        "bierner.markdown-mermaid",
        "bierner.markdown-preview-github-styles",
        "bierner.markdown-yaml-preamble",
        "donjayamanne.python-environment-manager",
        "esbenp.prettier-vscode",
        "GitHub.copilot",
        "GitHub.vscode-github-actions",
        "GitHub.vscode-pull-request-github",
        "ms-python.black-formatter",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "psioniq.psi-header",
        "redhat.vscode-yaml",
        "samuelcolvin.jinjahtml",
        "yzhang.markdown-all-in-one",
        "Gruntfuggly.todo-tree"
      ]
    }
  }
}
