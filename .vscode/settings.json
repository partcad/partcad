{
  "partcad.packagePath": "examples",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 2
  },
  "psi-header.config": {
    "author": "PartCAD",
    "authorEmail": "support@partcad.org",
    "blankLinesAfter": 1,
    "company": "PartCAD",
    "copyrightHolder": "PartCAD",
    "creationDateZero": "asIs",
    "forceToTop": true,
    "hostname": "https://partcad.org/",
    "license": "Apache License, Version 2.0",
    "spacesBetweenYears": false
  },
  "psi-header.lang-config": [
    {
      "begin": "",
      "end": "",
      "language": "python",
      "prefix": "# "
    }
  ],
  "psi-header.templates": [
    {
      "language": "python",
      "template": [
        "",
        "PartCAD, <<dateformat('YYYY')>>",
        "",
        "Author: <<author>> (<<authorEmail>>)",
        "Created: <<date>>",
        "",
        "Licensed under Apache License, Version 2.0.",
        "",
      ]
    }
  ],
  "files.trimFinalNewlines": true,
  "files.encoding": "utf8",
  "editor.useTabStops": false,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.autoIndent": "full",
  "editor.rulers": [
    120
  ],
  "files.exclude": {
    ".git/**": true,
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/.pytest_cache": true,
    "**/apache20.svg": true,
    "**/LICENSE.txt": true,
    ".ocp_vscode": true,
    ".conda": true,
    ".venv": true,
    ".tox": true,
    "**/.partcad": true,
    "docs/build": true,
    "partcad*/build": true,
    "partcad*/dist": true
  },
  "search.exclude": {
    ".git/**": true,
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/.pytest_cache": true,
    "**/apache20.svg": true,
    "**/LICENSE.txt": true,
    ".ocp_vscode": true,
    ".conda": true,
    ".venv": true,
    ".tox": true,
    "**/.partcad": true,
    "docs/build": true,
    "partcad*/build": true,
    "partcad*/dist": true
  },
  "markdown.extension.toc.omittedFromToc": {
    "README.md": [
      "# PartCAD"
    ]
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.testing.pytestArgs": [],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.terminal.executeInFileDir": true,
  "python.terminal.launchArgs": [],
  // TODO(clairbee): "python.analysis.typeCheckingMode": "basic",
  "OcpCadViewer.advanced.autostart": false,
  "yaml.format.bracketSpacing": false, // prevent YAML formatter from breaking Jinja2 templates
  "esbonio.sphinx.confDir": "",
  "files.eol": "\n",
  "prettier.proseWrap": "always",
  "prettier.printWidth": 120,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.envFile": "${workspaceFolder}/.env",
  "cSpell.words": [
    "coderabbitai",
    "gource",
    "libx",
    "manylinux",
    "mkdocs",
    "NOCOMMIT",
    "pfscad",
    "pfstl",
    "pipefail",
    "pyproject",
    "Regen",
    "renderable",
    "ruamel",
    "ultrafast",
    "vcodec"
  ],
}
