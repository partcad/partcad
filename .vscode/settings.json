{
  "esbonio.sphinx.confDir": "docs/source",
  "esbonio.server.enableDevTools": true,
  "esbonio.server.showDeprecationWarnings": true,
  "esbonio.sphinx.srcDir": "docs/source",
  "esbonio.sphinx.forceFullBuild": true,
  "esbonio.sphinx.keepGoing": true,
  "esbonio.server.logLevel": "debug",
  "esbonio.server.pythonPath": "/workspaces/partcad/partcad-cli/.venv/bin/python3.10",
  "esbonio.server.updateBehavior": "promptAlways",
  "esbonio.server.updateFrequency": "daily",
  "python.analysis.extraPaths": ["./partcad/src"],
  "cSpell.enabledFileTypes": {
    "feature": true
  },
  "psi-header.templates": [
    {
      "language": "python",
      "template": [
        "",
        "OpenVMP, 2023-2024",
        "",
        "Author: <<author>> (<<authorEmail>>)",
        "Created: <<date>>",
        "",
        "Licensed under Apache License, Version 2.0.",
        "",
      ]
    }
  ],
  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "",
      "end": "",
      "prefix": "# "
    }
  ],
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1,
    "spacesBetweenYears": false,
    "license": "Apache License, Version 2.0",
    "author": "Aleksandr Ilin",
    "initials": "AI",
    "authorEmail": "ailin@partcad.org",
    "company": "PartCAD Inc.",
    "copyrightHolder": "PartCAD Inc.",
    "creationDateZero": "asIs",
    "hostname": "https://partcad.org/"
  },
  // "psi-header.variables": [
  //   ["projectName", "OpenVMP"],
  //   ["projectCreationYear", "2023"]
  // ]
}
