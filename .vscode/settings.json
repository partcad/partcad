{
  // TODO(clairbee): "python.analysis.typeCheckingMode": "basic",
  "OcpCadViewer.advanced.autostart": false,
  "[feature]": {
    // Need to find a way to keep formatting in """
    "editor.formatOnSave": false
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "editor.insertSpaces": false
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "amazonQ.importRecommendationForInlineCodeSuggestions": false,
  "amazonQ.shareContentWithAWS": false,
  "amazonQ.showInlineCodeSuggestionsWithCodeReferences": false,
  "amazonQ.telemetry": false,
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.enabled": true,
  "cSpell.enabledFileTypes": {
    "feature": true
  },
  "cSpell.words": [
    "coderabbitai"
  ],
  "codeQL.telemetry.enableTelemetry": true,
  "codeium.enableConfig": {
    "*": true,
    "feature": true,
    "log": true,
    "markdown": true
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "dotfiles.repository": "deep-space-cartel/dotfiles",
  "editor.accessibilitySupport": "off",
  "editor.autoIndent": "full",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Hack Nerd Font Mono'",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "esbonio.server.enableDevTools": true,
  "esbonio.server.logLevel": "debug",
  "esbonio.server.pythonPath": "/workspaces/partcad/partcad-cli/.venv/bin/python3.10",
  "esbonio.server.showDeprecationWarnings": true,
  "esbonio.server.updateBehavior": "promptAlways",
  "esbonio.server.updateFrequency": "daily",
  "esbonio.sphinx.confDir": "docs/source",
  "esbonio.sphinx.forceFullBuild": true,
  "esbonio.sphinx.keepGoing": true,
  "esbonio.sphinx.srcDir": "docs/source",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/*.egg-info": true,
    "**/.partcad": true,
    "**/.pytest_cache": true,
    "**/LICENSE.txt": true,
    "**/__pycache__": true,
    "**/apache20.svg": true,
    ".conda": true,
    ".git/**": true,
    ".ocp_vscode": true,
    ".tox": true,
    ".venv": true,
    "docs/build": true,
    "partcad*/build": true,
    "partcad*/dist": true
  },
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "githubPullRequests.pullBranch": "never",
  "go.toolsManagement.autoUpdate": true,
  "markdown.extension.toc.omittedFromToc": {
    "README.md": [
      "# PartCAD"
    ]
  },
  "openapi.approvedHostnames": [],
  "partcad.packagePath": "examples",
  "partcad.pythonSandbox": "pypy",
  "prettier.printWidth": 120,
  "prettier.proseWrap": "always",
  "psi-header.config": {
    "author": "PartCAD",
    "authorEmail": "support@partcad.org",
    "blankLinesAfter": 1,
    "company": "PartCAD",
    "copyrightHolder": "PartCAD",
    "creationDateZero": "asIs",
    "forceToTop": true,
    "hostname": "https://partcad.org/",
    "license": "Apache License, Version 2.0",
    "spacesBetweenYears": false
  },
  "psi-header.lang-config": [
    {
      "begin": "",
      "end": "",
      "language": "python",
      "prefix": "# "
    }
  ],
  "psi-header.templates": [
    {
      "language": "python",
      "template": [
        "",
        "PartCAD, <<dateformat('YYYY')>>",
        "",
        "Author: <<author>> (<<authorEmail>>)",
        "Created: <<date>>",
        "",
        "Licensed under Apache License, Version 2.0.",
        "",
      ]
    }
  ],
  "python.analysis.extraPaths": [
    "./partcad/src"
  ],
  "python.envFile": "${workspaceFolder}/.env",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.executeInFileDir": true,
  "python.terminal.launchArgs": [],
  "python.testing.pytestArgs": [],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "redhat.telemetry.enabled": false,
  "remote.autoForwardPortsSource": "hybrid",
  "search.exclude": {
    "**/*.egg-info": true,
    "**/.partcad": true,
    "**/.pytest_cache": true,
    "**/LICENSE.txt": true,
    "**/__pycache__": true,
    "**/apache20.svg": true,
    ".conda": true,
    ".git/**": true,
    ".ocp_vscode": true,
    ".tox": true,
    ".venv": true,
    "docs/build": true,
    "partcad*/build": true,
    "partcad*/dist": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "svg.preview.mode": "svg",
  "terminal.integrated.fontFamily": "'Hack Nerd Font Mono'",
  "terminal.integrated.scrollback": 10000000,
  "testing.automaticallyOpenPeekView": "never",
  "testing.followRunningTest": false,
  "testing.openTesting": "neverOpen",
  "workbench.colorTheme": "GitHub Dark",
  "yaml.format.bracketSpacing": false // prevent YAML formatter from breaking Jinja2 templates
}
