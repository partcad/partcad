  "version": "0.2.0",
  "configurations": [
    //
    {
      "name": "Debug: partcad behavex",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/bin/behavex",
      "args": [
        // TODO: @alexanderilyin: use $input to make it possible to run any feature file
        "--parallel-processes=2",
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      // "env": {
        // "PATH": "${workspaceFolder}/.venv/bin/:${env:PATH}",
        // "PYTHONPATH": "${env:PATH}"
      // }
      "justMyCode": false
    },
    {
      "name": "Debug: partcad behave",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/bin/behave",
      "args": [
        // TODO: @alexanderilyin: use $input to make it possible to run any feature file
        "features/partcad-cli/pc.feature",
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PATH": "${workspaceFolder}/.venv/bin/:${env:PATH}",
        "PYTHONPATH": "${env:PATH}"
      }
    },
    // partcad -p examples inspect --package /produce_part_cadquery_primitive :cube
    {
      "name": "Debug: partcad inspect",
      "type": "debugpy",
      "request": "launch",
      "program": "partcad-cli/src/partcad_cli/click/command.py",
      "args": [
        "-p",
        "examples",
        "inspect",
        "--package",
        "/produce_part_cadquery_primitive",
        ":cube"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${env:PATH}"
      },
      "justMyCode": false
    },
    // partcad -p /workspaces/partcad/examples render --package /produce_assembly_assy -t readme -O ./ -a :logo_embedded
    {
      "name": "Debug: partcad render",
      "type": "debugpy",
      "request": "launch",
      "program": "partcad-cli/src/partcad_cli/click/command.py",
      "args": [
        "-p",
        "/workspaces/partcad/examples",
        "render",
        "--package",
        "/produce_assembly_assy",
        "-t",
        "readme",
        "-O",
        "./",
        "-a",
        ":logo_embedded",
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${env:PATH}"
      }
    },
    {
      "name": "Debug: partcad status",
      "type": "debugpy",
      "request": "launch",
      "program": "partcad-cli/src/partcad_cli/click/command.py",
      "args": [
        "status"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${env:PATH}"
      }
    },
    {
      "name": "Debug: partcad init",
      "type": "debugpy",
      "request": "launch",
      "program": "partcad-cli/src/partcad_cli/click/command.py",
      "args": [
        "init"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${env:PATH}"
      }
    },
    {
      "name": "Debug: partcad add --help",
      "type": "debugpy",
      "request": "launch",
      "program": "partcad-cli/src/partcad_cli/click/command.py",
      "args": [
        "add",
        "--help"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${env:PATH}"
      }
    }
  ]
