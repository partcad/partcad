        },
        {
          "command": "partcad.exportTo3MF",
          "when": "view == partcadExplorer"
        },
        {
          "command": "partcad.exportToThreeJS",
          "when": "view == partcadExplorer"
        },
        {
          "command": "partcad.exportToOBJ",
          "when": "view == partcadExplorer"
        },
        {
